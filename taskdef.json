{
    "family": "getveeya-api",
    "containerDefinitions": [
        {
            "image": "381960425321.dkr.ecr.us-east-1.amazonaws.com/getveeya-api:v_%BUILD_NUMBER%",
            "name": "getveeya-api",
            "cpu": 0,
            "memoryReservation": 400,
            "essential": true,
            "portMappings": [
                 {
                   "hostPort": 0,
                   "containerPort": 3000,
                   "protocol": "tcp"
                 }
            ],           
            "environment" : [
                { "name" : "MONGO_URL", "value" : "mongodb://spencer:ecbyankee25@ds221990.mlab.com:21990/veeya" }
            ],
            "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-group": "getveeya-api",
                "awslogs-region": "us-east-1"
              }
            }
        }
    ]
}